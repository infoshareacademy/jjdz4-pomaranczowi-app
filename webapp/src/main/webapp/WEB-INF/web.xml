<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>

  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>com.infoshareacademy.pomaranczowi.financialanalyser.servlets.HelloServlet</servlet-class>
  </servlet>
    <servlet>
        <servlet-name>HelloPost</servlet-name>
        <servlet-class>com.infoshareacademy.pomaranczowi.financialanalyser.servlets.HomePageServlet</servlet-class>
    </servlet>

  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HelloPost</servlet-name>
        <url-pattern>/post</url-pattern>
    </servlet-mapping>

</web-app>
